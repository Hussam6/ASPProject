@model Project.Models.WeatherModels.LatLngModel

@{
    ViewBag.Title = "WeatherForm";
}
<link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
<h2>Search for a location</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary()

    <p>Lat: @Html.TextBoxFor(x => x.lat) </p>
    <p>lon: @Html.TextBoxFor(x => x.lng) </p>

    <input class="btn btn-primary" type="submit" />
    <input class="btn btn-primary" value="Go Back" onclick="location.href='@Url.Action("Index", "Home")'" />
}




